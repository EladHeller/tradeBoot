{
  "manifest_version": 2,
  "name": "wiki boot",
  "version": "0.0.0.1",
  "permissions": [
          "<all_urls>",
		  "storage",
		  "unlimitedStorage"
        ],
	"background": {
		"persistent":false,
		"scripts": ["main.js"]
  },
  "web_accessible_resources": [
    "utils.js", "WikiParser.js", "WikiTableParser.js", "WikiUpdater.js", "Company.js", "TradeTemplateBoot.js"
  ]
}